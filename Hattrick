# cook your dish here
for t in range(int(input())):
    a, b, c, d, e, f = map(str,input().split())
    if (a == 'W' and b == 'W' and c == 'W') or (b == 'W' and c == 'W' and d == 'W') or (c == 'W' and d == 'W' and e == 'W') or (d == 'W' and e == 'W' and f == 'W'):
        print("YES")
    else:
        print("NO")
